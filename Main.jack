class Main {
    function void main() 
    {
        var Snake snake;
        let snake = Snake.new();
        do snake.drawSnake();
        do snake.growSnake();
        do snake.eraseSnake();
        do snake.drawSnake();
        do snake.nextPos();
        do snake.eraseSnake();
        do snake.drawSnake();
        return;
    }
}
